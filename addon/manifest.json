{
    "manifest_version": 2,
    "name": "Moera",
    "version": "0.1.0",
    "description": "Moera Decentralized Social Network.",

    "icons": {
        "32": "icons/moera-32.png",
        "64": "icons/moera-64.png",
        "128": "icons/moera-128.png"
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    }
}
